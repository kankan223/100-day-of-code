
a = 1        
a1 = 1         # variable to input number
b = 'x'           # variable to input a single character
c = "Developer"     # variable to input string
d = True               # variable to input a boolean value
e = complex(8, 5)         # complex function is used to write a complex number

print (a)
print (b)
print (c)
print (d)
print (e)

print (a + a1)    #use of variable to do arithmetic

print ("The type of a is", type(a))   #type is used to find the type of the variable

       
